local HttpService = game:GetService("HttpService")

local BACKEND_SEND_URL = "https://backend-vercel-ashy.vercel.app/api/send.js" -- ganti sesuai domainmu
local WEBHOOK_URL = "https://discord.com/api/webhooks/1378086156624990361/8qHKxSBQ8IprT1qFn1KkHDWsyRfKXPJkS_4OYzMkBC-PIhGClm0v36uIgzrVwtU1zXh6" -- ganti dengan webhook kamu

local embed = {
    title = "ðŸš€ Test Embed from Roblox",
    description = "Ini adalah pesan embed yang dikirim dari Roblox ke Discord lewat backend proxy.",
    color = 0x1abc9c,
    fields = {
        {name = "Field 1", value = "Ini isi field 1", inline = true},
        {name = "Field 2", value = "Ini isi field 2", inline = true}
    },
    timestamp = os.date("!%Y-%m-%dT%H:%M:%SZ"),
    footer = { text = "Roblox Logger" }
}

local payload = {
    url = WEBHOOK_URL,
    embeds = { embed }
}

local success, response = pcall(function()
    return HttpService:RequestAsync({
        Url = BACKEND_SEND_URL,
        Method = "POST",
        Headers = {
            ["Content-Type"] = "application/json"
        },
        Body = HttpService:JSONEncode(payload)
    })
end)

if success then
    print("Webhook sent successfully")
else
    warn("Failed to send webhook:", response)
end
