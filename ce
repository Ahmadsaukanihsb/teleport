local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

print("✅ Script dimulai") -- Harus muncul di output!

local function acceptGiftTest()
    print("🔄 Mencoba menerima gift...")

    local player = Players.LocalPlayer
    if not player then
        print("❌ LocalPlayer tidak ditemukan")
        return
    end

    local giftsFolder = player:FindFirstChild("Gifts") or player:FindFirstChild("ReceivedGifts")
    if not giftsFolder then
        print("📭 Tidak ada folder Gifts/ReceivedGifts")
        return
    end

    print("🎁 Ditemukan folder gift dengan", #giftsFolder:GetChildren(), "item")

    for _, gift in ipairs(giftsFolder:GetChildren()) do
        print("🧾 Gift:", gift.Name)
    end
end

task.wait(3) -- Tunggu world siap
acceptGiftTest()
