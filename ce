local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

print("âœ… Script dimulai") -- Harus muncul di output!

local function acceptGiftTest()
    print("ğŸ”„ Mencoba menerima gift...")

    local player = Players.LocalPlayer
    if not player then
        print("âŒ LocalPlayer tidak ditemukan")
        return
    end

    local giftsFolder = player:FindFirstChild("Gifts") or player:FindFirstChild("ReceivedGifts")
    if not giftsFolder then
        print("ğŸ“­ Tidak ada folder Gifts/ReceivedGifts")
        return
    end

    print("ğŸ Ditemukan folder gift dengan", #giftsFolder:GetChildren(), "item")

    for _, gift in ipairs(giftsFolder:GetChildren()) do
        print("ğŸ§¾ Gift:", gift.Name)
    end
end

task.wait(3) -- Tunggu world siap
acceptGiftTest()
