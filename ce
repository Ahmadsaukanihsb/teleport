local ReplicatedStorage = game:GetService("ReplicatedStorage")
local HttpService = game:GetService("HttpService")

local possibleRemoteFunctionNames = {
    "PetService",
    "PetGiftingService",
    "GiftService",
    "GiftRemoteFunction",
    "InboxService",
    "GiftData",
    "GetGift",
    "GetGifts",
    "GetInbox"
}

for _, name in ipairs(possibleRemoteFunctionNames) do
    local remote = ReplicatedStorage:FindFirstChild(name)
    if remote and remote:IsA("RemoteFunction") then
        print("üîç RemoteFunction ditemukan:", remote.Name)
        
        local success, result = pcall(function()
            return remote:InvokeServer()
        end)

        if success then
            print("‚úÖ Hasil dari", remote.Name, ":")
            if typeof(result) == "table" then
                print(HttpService:JSONEncode(result))
            else
                print(result)
            end
        else
            warn("‚ùå Gagal InvokeServer pada", remote.Name, "-", result)
        end
    elseif remote and remote:IsA("RemoteEvent") then
        print("‚ÑπÔ∏è RemoteEvent ditemukan (bukan RemoteFunction):", remote.Name)
    end
end

print("‚úÖ Selesai mengecek semua kemungkinan RemoteFunction.")
